public class StopEvent extends Event {
	private double time;
	private State s;
	private String eventName;
	
	public StopEvent(State s) {
		time = 999;
		this.s = s;
		
		
	}
	void effekt() {
		
		s.setCurrentEvent(this);
		s.stop();
		
	}

	
	public double getTime() {
	
		return time;
	}
	public String getEventName() {
		return eventName;
	}
	public int getCustomerNr() {
		return 0;
	}

}
